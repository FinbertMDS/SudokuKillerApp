{
  "name": "killer-sudoku",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:build:aab": "cd android && ./gradlew bundleRelease",
    "ios:archive": "cd ios && xcodebuild -workspace SudokuKillerApp.xcworkspace -scheme SudokuKillerApp -configuration Release -sdk iphoneos -archivePath build/SudokuKillerApp.xcarchive archive && mkdir -p ~/Library/Developer/Xcode/Archives/$(date +%Y-%m-%d) && mv build/SudokuKillerApp.xcarchive ~/Library/Developer/Xcode/Archives/$(date +%Y-%m-%d)/SudokuKillerApp-$(date +%H-%M-%S).xcarchive"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.3.10",
    "axios": "^1.9.0",
    "date-fns": "^4.1.0",
    "i18next": "^25.0.2",
    "killer-sudoku-generator": "^1.1.2",
    "react": "19.0.0",
    "react-i18next": "^15.5.1",
    "react-native": "0.79.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-device-info": "^14.0.4",
    "react-native-dotenv": "^3.4.11",
    "react-native-google-mobile-ads": "^15.2.0",
